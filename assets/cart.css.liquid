@media (max-width: 1024px) {
  .cartTitle {
      text-align: center;
  }
  .cartContent form {
    flex-direction: column;
  }
  .cartTable, .functionCart.flexRow {
    width: 100%;
  }
  .functionCart.flexRow {
      max-width: 500px;
      margin: 0 auto;
      width: 100%;
  }
  .cartContent form .cartItemWrap {
      margin: 0 auto;
  }
}
@media (max-width: 769px) {
  .template-cart #pageContent {
      padding: 0 20px;
  }
  .template-cart .cartItemWrap .productInfo {
      padding: 10px!important;
  }
  .template-cart .cartItemWrap .productInfo .ebyCartItemFooter {
      margin-top: 2rem;
  }
  .cartContainer .functionCartButton, .cartContainer .price_block {
      padding: 0;
  }
  .cartContainer .functionCartButton, .cartContainer .price_block {
      padding: 0;
  }
}
