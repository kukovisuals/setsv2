function detectMob() {
    const toMatch = [
        /Android/i,
        /webOS/i,
        /iPhone/i,
        /iPad/i,
        /iPod/i,
        /BlackBerry/i,
        /Windows Phone/i
    ];

    return toMatch.some((toMatchItem) => {
        return navigator.userAgent.match(toMatchItem);
    });
} 

function runVideo(){
   var heroVideo = document.getElementById('homepage-vid');
     // desktop- https://cdn.shopify.com/videos/c/o/v/8eb2da1f53174368865568dd45eb4aa5.mp4
