@media (max-width: 1024px) {
  section.foot_section .rowFlex.rowFlexMargin>.col-md-8+.col-md-4 {
      width: 100%;
      padding: 4rem 2rem 2rem;
      border-top: solid 1px #000;
      margin-top: 50px;
  }
  section.foot_section .rowFlex.rowFlexMargin>.col-md-8 {
      width: 100%;
  }
  .menu_section {
      float: none;
      border-right: none;
      max-width: 660px;
      border-bottom: none;
      padding: 0;
      margin: 0 auto;
      clear: both;
      overflow: hidden;
  }
  .menu_section .menu_col1, .menu_section .menu_col2, .menu_section .menu_col3 {
      float: left;
      width: 33.3%;
  }
  .cont_section {
    padding-left: 0;
  }
  .footerCopyRightInner .rowFlexMargin {
      margin-left: 0;
      margin-right: 0;
      display: flex;
      flex-wrap: wrap;
      width: 100%;
  }
  .footerCopyRightInner .rowFlex>div:first-child {
      order: 3;
      width: 100%;
      text-align: center;
      border-top: 1px solid #d5d5d5;
      padding-top: 20px;
      margin-top: 10px;
      padding-bottom: 10px;
  }
  .footerCopyRightInner .rowFlex>div {
    width: 100%;
    text-align: center;
    padding-bottom: 1rem;
  }
  #velaFooter.style1 .velaCopyRightLink {
      float: right;
  }
  .ebySitemapLinkWrapper {
      margin: 0 auto;
      width: 50%;
  }
  .ebyGuideChartWrapper .ebyDesktop-guideWrapper{
      display: none;
  }
  .ebyGuideChartWrapper .ebyTablet-guideWrapper {
      display: block;
  }
  span.colReviewAggrWrapper {
    font-size: 1.2rem;
  }
  .reviewStar {
    font-size: 5px;
  }
  .slick-list .reviewStar {
    font-size: 7px;
  }
}
@media (max-width: 769px) {

  section.foot_section {
	  padding: 40px 0 10px 0;
  }
  .menu_section .menu_col1, body .menu_section .menu_col2, body .menu_section .menu_col3 {
      width: 100%;
  }
  .menu_section .header {
    margin: 0 0 10px 0;
    padding: 0 10px;
  }
  .menu_section ul {
      padding: 0 10px 30px 10px;
      display: flex;
      flex-wrap: wrap;
      border-bottom: 1px solid;
      margin-bottom: 30px;
  }
  .menu_section ul li {
      width: 50%;
      display: inline-block;
  }
  .menu_section ul.about_menu {
      border-bottom: none;
  }
  .footerCopyRightInner .rowFlex>div:nth-child(3) {
      border-top: 1px solid #d5d5d5;
      padding-top: 20px;
  }
  #velaFooter.style1 .velaCopyRightLink {
      float: none;
  }
  #velaFooter.style1 .velaCopyRightLink ul {
      flex-wrap: wrap;
      text-align: center;
  }
  #velaFooter.style1 .velaCopyRightLink ul li:not(:last-child), 
  #velaFooter.style1 .velaCopyRightLink ul li {
    width: 100%;
    display: block;
    padding: 0;
    margin: 0;
  }
  /**/
  .pop_inner .pop_wrap {
    max-width: 90%;
  }
  span.colz_pop {
      top: 45px;
      right: 45px;
  }
  .pop_size .ebyDesktop-guideWrapper {
      display: none;
  }
  /**/
  .ebyGuideChartWrapper .ebyMobile-guideWrapper {
      display: block;
  }
  /**/
  .pop_inner .pop_wrap {
    max-width: 90%;
  }
  .ajaxCartInner .drawerCartFooter,
  #cartContainer .drawerCartFooter{
    max-width: 600px;
  }

}
@media (max-width: 480px) {
  #accountPopup, #paymentPopup, #shippingAddressPopup, #billingAddressPopup, #paymentPopup, #changeMyBoxPopup, #changeMyPrefsPopup {
    max-height: 416px;
    padding: 1rem;
  }
  .drawer {
      max-width: 600px;
  }
