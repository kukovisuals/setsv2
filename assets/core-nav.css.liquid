 .eby-header nav.eby-header-nav .mobileNavMenuDropdown {
	 -o-transition: 0.2s ease-in-out all;
	 -moz-transition: 0.2s ease-in-out all;
	 -webkit-transition: 0.2s ease-in-out all;
	 transition: 0.2s ease-in-out all;
	 -o-transform: rotate(45deg);
	 -moz-transform: rotate(45deg);
	 -webkit-transform: rotate(45deg);
	 transform: rotate(45deg);
	 font-size: 1.75rem;
	 width: 25px;
	 height: 25px;
	 line-height: 1rem;
	 margin-left: 20px;
	 display: block;
	 padding: 0;
}
 .eby-header nav.eby-header-nav .mobileNavMenuDropdown.mini {
	 font-size: 1.5rem;
	 width: 22px;
	 height: 22px;
	 line-height: 1rem;
	 margin-left: 25px;
}
 .eby-header nav.eby-header-nav .mobileNavMenuDropdown.mini.open:before {
	 width: 19px;
}
 .eby-header nav.eby-header-nav .mobileNavMenuDropdown.mini.open:after {
	 content: "";
	 display: none;
}
 .eby-header nav.eby-header-nav .mobileNavMenuDropdown.open {
	 -o-transform: rotate(0);
	 -moz-transform: rotate(0);
	 -webkit-transform: rotate(0);
	 transform: rotate(0);
	 padding: 0;
}
 .eby-header nav.eby-header-nav .mobileNavMenuDropdown.open:before {
	 content: " ";
	 background: #0064ff;
	 height: 1px;
	 width: 20px;
}
 .eby-header nav.eby-header-nav .mobileNavMenuDropdown.open:after {
	 content: "";
	 display: none;
}
