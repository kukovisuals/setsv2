    {% unless page.handle == 'build-your-box' %}
    <section class="newslsec newslsec-{{ template }}">
      {% section 'new-letter-section' %}
    </section>
    {% section 'vela-footer' %}
    {% include 'vela-notification' %}
    {% endunless %}
