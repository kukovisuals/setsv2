<script defer type="text/javascript">
  
  // Load the IFrame Player API code asynchronously.
  var tag = document.createElement('script');
  tag.src = "https://www.youtube.com/player_api";
  var firstScriptTag = document.getElementsByTagName('script')[0];
  firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
  
  // Replace the 'ytplayer' element with an <iframe> and
  // YouTube player after the API code downloads.
  var player;
  function onYouTubePlayerAPIReady() {
    player = new YT.Player('playerDesktop', {
        height: '400',
        width: '400',
        videoId: '{{ page.metafields.ebyacf.page_banner_video_header }}',
        playerVars: {
        'modestbranding':1,
        'origin': 'https://shop.join-eby.com',
        'controls': 0
        },
        events: {
        'onReady': onPlayerReady,
        'onStateChange': function(e) {
            if (e.data === YT.PlayerState.ENDED) {
              e.target.playVideo(); 
            }
          }
        }
    });
    
  }
  var onPlayerReady = function (event) {
    var embedCode = event.target.getVideoEmbedCode();
    console.log(':: yt player fire ::', embedCode);
    if (!!event.target) {
      setTimeout(function() {
        event.target.mute()
      	event.target.playVideo();
        // hide placeholder
        document.getElementById('feelsLikeThisPlaceholderImage').className += " playerImgHide";
      }, 200);
    }
  };
  
  document.addEventListener('DOMContentLoaded', function (){
    
    jQuery(document).on('click', '.siz_guid', function(){
      if(jQuery('.pop_size.size_guid_pdp').is(':visible')) {
        jQuery('.pop_size.size_guid_pdp').fadeOut()
      } else {
        if ($(this).closest('.proVariantForm').hasClass('pantyForm')) {
          $('.pop_size.size_guid_pdp .ebyBraletteGuide').hide();
          $('.pop_size.size_guid_pdp .ebyPantyGuide').show();
        } else if ($(this).closest('.proVariantForm').hasClass('braletteForm')) {
          $('.pop_size.size_guid_pdp .ebyPantyGuide').hide();
          $('.pop_size.size_guid_pdp .ebyBraletteGuide').show();
        }
        jQuery('.pop_size.size_guid_pdp').fadeIn();
      }
      return false;
    });
    
    $('body').on('click', '.scroll_btn', function(e) {
      e.preventDefault();
      $('html, body').animate({
        scrollTop: $('#'+e.target.getAttribute('data-scrollto')).offset().top - 100
      }, 1000);
    });
    
    function checkScrollTop() {
      var header = $(".eby-header");

      if ($(window).scrollTop() > 0) {
        header.addClass("scrolled");
      } else {
        header.removeClass("scrolled");
      }
    }
    
    var isHomepage = $("body").hasClass("premium_news");

    if (isHomepage) {
      $(window).scroll(function () {
        checkScrollTop();
      });
    }

    function onPageLoad() {
      if (isHomepage) {
        checkScrollTop();
      }
    }

  });
</script>