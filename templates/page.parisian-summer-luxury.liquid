<script type="text/javascript" defer>
  document.addEventListener('DOMContentLoaded', function () {
    
    $('body').on('click', '.scroll_btn', function(e) {
      e.preventDefault();
      $('html, body').animate({
        scrollTop: $('#'+e.target.getAttribute('data-scrollto')).offset().top - 100
      }, 1000);
    });

    $('ul.tabs li').click(function(){
      var $this = $(this);
      var $theTab = $(this).attr('id');

      if($this.hasClass('active')){
      }else{
        $this.closest('.premium_tab_section').find('ul.tabs li').removeClass('active');
        $this.closest('.premium_tab_section').find('.premium_tab_body .tab-content').removeClass('active');
        $('ul.tabs li[id="'+$theTab+'"]').addClass('active');
        $('.premium_tab_body .tab-content[data-tab="'+$theTab+'"]').addClass('active');
      }

    });


    if ($(window).width() < 769) {
      $(".tab_proVariants a.tab_btn").click(function(event){
        event.preventDefault();
        $(this).next('.tab_proVariants_wrap').addClass('active');
        $(this).parent().next('.tab_proVariants_ovrlay').addClass('active');
      });

      $(".tab_proVariants_ovrlay, .tab_close").click(function(event){
        event.preventDefault();
        $('.tab_proVariants_wrap').removeClass('active');
        $('.tab_proVariants_ovrlay').removeClass('active');
      });
    }
    
    function checkScrollTop() {
      var header = $(".eby-header");

      if ($(window).scrollTop() > 0) {
        header.addClass("scrolled");
      } else {
        header.removeClass("scrolled");
      }
    }

    var isHomepage = $("body").hasClass("premium_news");

    if (isHomepage) {
      $(window).scroll(function () {
        checkScrollTop();
      });
    }

    function onPageLoad() {
      if (isHomepage) {
        checkScrollTop();
      }
    }

  });
</script>