            <span>{{ prodFabricTitle| capitalize  }}</span>
            <span>{{ prodNameTitle | capitalize  }}</span>
            <span class="color">{{ prodColorTitle | capitalize  }}</span>
          </h3>
          {% endif %}
        </div>
      </a>
