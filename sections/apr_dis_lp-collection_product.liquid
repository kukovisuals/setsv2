<div class="cotton-two image-grid mobile_grid">
  
  <div class="container">
    <div class="cotton-twos">
      {% for i in (2..2) %}
      {% capture img %}banner_image_{{i}}{% endcapture %}
      {% capture url %}banner_url_{{i}}{% endcapture %}
      {% capture title %}banner_title_{{i}}{% endcapture %}
      {% if section.settings[img] != blank %}
      <a href="{% if section.settings[url] != blank %}{{ section.settings[url] }}{% else %}#{% endif %}" class="cotton-left">
        <img width="490" height="533" data-src="{{ section.settings[img] | img_url: 'original' }}" class="lazyload" alt="{{ section.settings[img].alt }}">
        {% if section.settings[title] != blank %}
        <div class="cotton_overlay_text">
          {{ section.settings[title] }}
        </div>
        {% endif %}
      </a>
      {% endif %}
      {% endfor %}
    </div>
  </div>
</div>

