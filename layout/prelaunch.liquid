  {% include "starapps-language-meta" %}
<!-- starapps_scripts_end -->

  {{ content_for_header }}
  
  {% render 'dataLayer-allPages' %}


  {% render 'shogun-head' %}
</head>
{%- if template contains 'customer' -%}
