      </div>
      <div class="cotton-left product-grd collection_group">
        <div class="products_cols collection_group_list quarters">
          {% assign collection = collections[section.settings.collection_1] %}

          {% for product in collection.products limit: 14 %}
          {% if product.type contains "Bralette" or product.type contains "Tank" %}
          	{% continue %}
          {% endif %}
          
          {% if product.type contains "Underwear" %}
          {% for prodTag in product.tags %}
          {% if prodTag contains "pantyType-" %}
          {%- assign prodNameTitle = prodTag | replace: "pantyType-", "" -%}
          {% endif %}
          {% if prodTag contains "pantyColor-" %}
          {%- assign prodColorTitle = prodTag | replace: "pantyColor-", "" | replace: "_", " " -%}
          {% endif %}
          {% if prodTag contains "newCustomBoxPrice-" %}
          {%- assign prodDealPrice = prodTag | replace: "newCustomBoxPrice-", "" | replace: "_", " " -%}
          {% endif %}
          {% endfor %}
          {% elsif product.type contains "Bralette" %}
          {%- assign prodNameTitle = "bralette" -%}
          {% for prodTag in product.tags %}
          {% if prodTag contains "braletteColor-" %}
          {%- assign prodColorTitle = prodTag | replace: "braletteColor-", "" | replace: "_", " " -%}
          {% endif %}
          {% if prodTag contains "newCustomBoxPrice-" %}
          {%- assign prodDealPrice = prodTag | replace: "newCustomBoxPrice-", "" | replace: "_", " " -%}
          {% endif %}
          {% endfor %}
          {% endif %}

          {% if prodNameTitle == "highwaisted_thong" %}
          {% assign prdImgFileName = "hwthong" %}
          {% else %}
          {% assign prdImgFileName = prodNameTitle %}
          {% endif %}

          {% assign premImgFileName = "PROD---" | append: prodColorTitle | replace: " ", "" | append: "---" | append: prdImgFileName | append: ".jpg" %}

          <div class="col products_col velaProBlock quickadd{% if prodNameTitle == "bralette"%} topProd{% endif %}" data-size-guid="{% render 'collection-selectors-charts', product_target: product %}">
            <div class="col-wrap velaProBlockInner">
              <div class="product-image proHImage d-flex flexJustifyCenter">
                <a class="proFeaturedImage" href="{{ product.url }}">
                  {% for image in product.images %}
                  	{%- assign itemNumber = forloop.index | modulo:5 -%}
                      {%- if itemNumber == 2 -%}
                          <img class="img-responsive lazyload" alt="{{ image.alt }}" title="{{ image.alt }}" data-src="{{ image.src | product_img_url: "1000x" }}" />
                      {%- endif -%}
                  {% endfor %}
<!--                   <div class="col-pro-details">
