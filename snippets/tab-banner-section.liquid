{% if section.settings.desk_img != blank %} 
<div class="tab_collection_bennor">
  <div class="tab_collection_bennor_wrap">
    <div class="tab_collection_img">
      {% if section.settings.desk_img != blank %}
      <img src="{{ section.settings.desk_img | img_url: 'master' }}" class="desktop_image" alt="{{ section.settings.desk_img.alt }}">
      {% endif %}
      {% if section.settings.ipad_img != blank %}
      <img src="{{ section.settings.ipad_img | img_url: 'master' }}" class="ipad_image" alt="{{ section.settings.ipad_img.alt }}">
      {% elsif section.settings.desk_img != blank %}
      <img src="{{ section.settings.desk_img | img_url: 'master' }}" class="ipad_image" alt="{{ section.settings.desk_img.alt }}">
      {% else %}
      {% endif %}
      {% if section.settings.mob_img != blank %}
      <img src="{{ section.settings.mob_img | img_url: 'master' }}" class="mobile_image" alt="{{ section.settings.mob_img.alt }}">
      {% elsif section.settings.desk_img != blank %}
      <img src="{{ section.settings.desk_img | img_url: 'master' }}" class="mobile_image" alt="{{ section.settings.desk_img.alt }}">
      {% else %}
      {% endif %}
    </div>
    <div class="tab_collection_details text_center">
      <div class="tab_collection_details_wrap">
        <div class="tab_collection_content">
          {% if section.settings.title != blank %}
          <h2>
            {{ section.settings.title }}
            <!--           <a href="{{ section.settings.button_link }}">{{ section.settings.title }}</a> -->
          </h2>
          {% endif %}
          {% if section.settings.sub_title != blank %}
          <p>{{ section.settings.sub_title }}</p>
          {% endif %}
        </div>
        {% if section.settings.button_link != blank and section.settings.button_text != blank %}
        <div class="section-btn">
          <a href="{{ section.settings.button_link }}">{{ section.settings.button_text }}</a>
        </div>
        {% endif %}
      </div>
    </div>
  </div>
</div>
{% endif %}