<div class="cotton-two product-grid premium_product_section">
  <div class="container">
    {% if section.settings.title != blank %}
    <div class="collection_group_head">
      <h2 class="text-center">{{ section.settings.title }}</h2>
    </div>
    {% endif %}
    <div class="cotton-twos">
      <div class="cotton-left product-grd collection_group fullwidth">
        <div class="products_cols collection_group_list">
        {% assign collection = collections[section.settings.collection_1] %}
        {% for product in collection.products limit: 2 %}
          {% if product.type contains "Bralette" %}
          	{%- assign prodNameTitle = "bralette" -%}
          	{% for prodTag in product.tags %}
          		{% if prodTag contains "braletteColor-" %}
          			{%- assign prodColorTitle = prodTag | replace: "braletteColor-", "" | replace: "_", " " -%}
          		{% endif %}
          		{% if prodTag contains "newCustomBoxPrice-" %}
          			{%- assign prodDealPrice = prodTag | replace: "newCustomBoxPrice-", "" | replace: "_", " " -%}
          		{% endif %}
          	{% endfor %}
          {% endif %}
          {% if product.type contains "Tank" %}
          	{%- assign prodNameTitle = "tank" -%}
          	{% for prodTag in product.tags %}
          		{% if prodTag contains "tankColor-" %}
          			{%- assign prodColorTitle = prodTag | replace: "tankColor-", "" | replace: "_", " " -%}
          		{% endif %}
          		{% if prodTag contains "newCustomBoxPrice-" %}
          			{%- assign prodDealPrice = prodTag | replace: "newCustomBoxPrice-", "" | replace: "_", " " -%}
          		{% endif %}
          	{% endfor %}
          {% endif %}

          {% assign prdImgFileName = prodNameTitle %}

          {% assign premImgFileName = prodColorTitle | replace: " ", "" | append: "---" | append: prdImgFileName | append: "---missy---tile_fr.jpg" %}
          {% if prodNameTitle == "bralette"%}
          	{% assign prodNameKey = "ajbralette" %}
          {% else %}
          	{% assign prodNameKey = prodNameTitle %}
          {% endif %}

          <div class="col products_col velaProBlock quickadd{% if prodNameTitle == "bralette" or prodNameTitle == "tank"%} topProd{% endif %}" data-size-guid="{% render 'collection-selectors-charts', product_target: product %}">
            <div class="col-wrap velaProBlockInner">
              <div class="product-image proHImage d-flex flexJustifyCenter">
                <a class="proFeaturedImage" href="{{ product.url }}">
                  {% for image in product.images %}
                  	{%- assign itemNumber = forloop.index | modulo:5 -%}
                      {%- if itemNumber == 2 -%}
                  		  {% assign featImg = "PROD---black--seamless--" | append: prodNameKey | append: "--missy---feat.jpg" %}
                          <img class="img-responsive lazyload" alt="{{ image.alt }}" title="{{ image.alt }}" data-src="{{ featImg | file_img_url : "500x500", crop: "center" }}" />
                      {%- endif -%}
                  {% endfor %}
                </a>
              </div>
              <div class="col-pro-details">
                <h4><a href="{{ product.url }}">{{ product.title | replace: " Cotton", "" }}</a></h4>
                <div class="priceWrapper">
                  {% if customer.tags contains 'Active Subscriber' %}
                  <p class="bfx-price">{{ product.price | money_without_trailing_zeros }}</p>
                  <p class="priceProduct vipHighlight bfx-price">{{ product.metafields.ebyacf.vip_product_price | replace: '/ ', '' }}</p>
                  {% else %}
                  <p class="bfx-price">{{ product.price | money_without_trailing_zeros }}</p>
                  <p class="priceProduct vipHighlight bfx-price">{{ product.metafields.ebyacf.vip_product_price | replace: '/ ', '' }}</p>
                  {% endif %}
                </div>
              </div>
              <div class="product-details proButton clearfix">
                <div class="quick_btn pre-btn">
                  <div class="productQuickAdd">
                    <button class="btn btnProduct btnProductQuickAdd" data-href="#velaQuickAdd-{{ product.id }}-1" data-handle="{{ product.handle }}" title="Quick Add +">
                      <i class="icons icon-eye"></i>
                      <span class="text">Quick Add +</span>
                    </button>
                    <div class="selection-wrapper" id="velaQuickAdd-{{ product.id }}-1">
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        {% endfor %}
