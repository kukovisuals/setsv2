<script defer type="text/javascript">

  document.addEventListener('DOMContentLoaded', function() {

    const videos = document.querySelectorAll('.autoplay-vid23');
  
    // Load videos and posters
    videos.forEach(async video => {
      // Fetch the video source and poster URLs from the data attributes
      const videoSrc = video.dataset.src;
    
      // Fetch the video file
      const response = await fetch(videoSrc);
      const blob = await response.blob();
      video.src = URL.createObjectURL(blob);
      
      // Add event listener to pause other videos when the current video starts playing
      video.addEventListener('play', () => {
        pauseOtherVideos(video, videos);
      });
    });

    // Create an Intersection Observer to detect when a video is visible on the screen
    const observer = new IntersectionObserver(
      (entries, observer) => {
        entries.forEach(entry => {
          if (entry.isIntersecting) {
            // Load the poster if it's not already loaded
            if (!entry.target.poster) {
              console.log('entry', entry)
              const posterSrc = entry.target.dataset.poster;
              entry.target.poster = posterSrc;
            }
            entry.target.play();
          } else {
            entry.target.pause();
          }
        });
      },
      { threshold: 0.5 }
    );
    
    // Observe each video element for visibility changes
    videos.forEach(video => observer.observe(video));
    
    // Create a Mutation Observer to detect when a new video starts playing and pause other videos
    videos.forEach(video => {
      const mutationObserver = new MutationObserver(mutations => {
        mutations.forEach(mutation => {
          if (
            mutation.type === 'attributes' &&
            mutation.attributeName === 'paused' &&
            !mutation.target.paused
          ) {
            pauseOtherVideos(mutation.target, videos);
          }
        });
      });
    
      mutationObserver.observe(video, { attributes: true, attributeFilter: ['paused'] });
    });
    
    function pauseOtherVideos(currentVideo, videos) {
      videos.forEach(video => {
        if (video !== currentVideo) {
          video.pause();
        }
      });
    }

