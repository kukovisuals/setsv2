<div class="eby-right-icon-wrpper-22">
  <div class="eby-right-icon-contianer-22">
    <div class="header_right eby-nav-laptop mst-right-flx">

      {%- if customer -%}

        {%- else -%}
          <button
            class="right_text desktop_only ebyMicroBtn joinNow{% if trueCartCount <= 0 %} join{%  else %} buy{% endif %} {{border_color}} {{theme_color}} "
            data-subscribe="default"
            title="Subscribe">
            <span class="">SUBSCRIBE</span>
          </button>

      {%- endif -%}

      {%- if customer -%}
        <a
          class="right_text sign_in_btn"
          href="/account"
          title="{{ 'layout.header.account' | t }}">
          <span class="eby-icon-menu-22 {{border_color}}">
            <svg
              width="24"
              height="24"
              viewBox="0 0 24 24"
              xmlns="http://www.w3.org/2000/svg">
              <g
                class="{{border_color}}-icon"
                stroke="{{stroke_svg}}"
                fill="none"
                fill-rule="evenodd">
                <path d="M11.998 12.5c2.313 0 4.403.786 5.937 1.98 1.565 1.216 2.562 2.86 2.562 4.517a1.5 1.5 0 0 1-1.5 1.503H5c-.414 0-.79-.168-1.06-.44A1.495 1.495 0 0 1 3.5 19c0-1.66.997-3.303 2.56-4.52 1.535-1.194 3.625-1.98 5.938-1.98z" />
                <circle
                  cx="12"
                  cy="6"
                  r="4.5" />
              </g>
            </svg>
          </span>
        </a>
      {%- else -%}
        <a
          class="right_text sign_in_btn"
          href="/account/login"
          title="Sign In">
          <span class="eby-icon-menu-22 {{border_color}}">
            <svg
              width="24"
              height="24"
              viewBox="0 0 24 24"
              xmlns="http://www.w3.org/2000/svg">
              <g
                class="{{border_color}}-icon"
                stroke="{{stroke_svg}}"
                fill="none"
                fill-rule="evenodd">
                <path d="M11.998 12.5c2.313 0 4.403.786 5.937 1.98 1.565 1.216 2.562 2.86 2.562 4.517a1.5 1.5 0 0 1-1.5 1.503H5c-.414 0-.79-.168-1.06-.44A1.495 1.495 0 0 1 3.5 19c0-1.66.997-3.303 2.56-4.52 1.535-1.194 3.625-1.98 5.938-1.98z" />
                <circle
                  cx="12"
                  cy="6"
                  r="4.5" />
              </g>
            </svg>

          </span>
        </a>
      {%- endif -%}



      <div class="right_text ebyHeaderCtaWrapper {{border_color}}">
        <button
          id="global-search-btn-desktop"
          name="q"
          class="ebyMiniSearch snize-input-style desktop_only">
          <span class="animate searchIconWrapper">
            <i class="ebyicon-search"></i>
          </span>
        </button>
        <button
          id="global-search-btn-mobile"
          name="q"
          class="ebyMiniSearch snize-input-style mobile_only">
          <span class="animate searchIconWrapper">
            <i class="ebyicon-search"></i>
          </span>
        </button>
        <a data-cymenu="cartIcon" class="cart-link jsDrawerOpenRight d-flex ebyMinicart {%if trueCartCount == 0 %}isEmpty{% endif %} {{theme_color}}" href="/cart">
          <span class="animate cartIconWrapper">
            <i class="ebyicon-minicart-empty-alt"></i>
            <span class="item-count {{theme_background}}">
              <span id="CartCount" class="{{cart_index}}">{{ trueCartCount }}</span>
            </span>
          </span>
        </a>
      </div>
    </div>
  </div>
</div>