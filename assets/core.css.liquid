/* account new */
@font-face {
    font-family: "Sang Bleu OG";
    src: local("Sang Bleu OG"),
      url("https://cdn.shopify.com/s/files/1/0313/4062/5964/files/SangBleuOGSans-Medium.otf?v=1682931705") format("opentype");
}

#account .dash-boxChoiceWrapper .sizeOptListing.capsuleMode {
  overflow: unset;
  width: 100%;
  flex-wrap: wrap;
  justify-content: space-evenly;
  max-width: 392px;
  align-items: center;
  margin: 0 auto;
}
#account .dash-boxChoiceWrapper .sizeOptListing.capsuleMode .sortOpt {
  margin: 0.8rem 0;
}
#account #changeMyBoxPopup h3, #account #accountPopup>h3, #account #paymentPopup .updatePaymentDetail>h3, #account #billingAddressPopup>h3, #account #shippingAddressPopup>h3, #account #updatePassword .updatePassword>h3 {
  font-family: 'Sang Bleu OG', sans-serif;
}
#account #changeMyPrefsPopup h3 {
  color: #000c48;
  line-height: normal;
  font-family: 'Sang Bleu OG', sans-serif;
  display: block;
  font-size: 2.4rem;
  padding: 1rem 0;
  margin: 0;
  padding: 1rem 0;
}
#account .dashOptWrapper input:disabled+label,
#account .dashBtnWrapper button.submitSelect[disabled] {
    background: #bfc1cb !important;
    color: #fff;
    border-color: #bfc1cb !important;
}
#account .dashBtnWrapper button.submitSelect {
    background: #4a90e2;
    width: 100%;
    max-width: 320px;
    border-radius: 4px;
    border: solid 1px #4a90e2;
    font-size: 1.2rem;
    line-height: inherit;
}
#account .dash-prefChoiceOptions.styleGroup .planOptListing .prefChoiceOption {
    max-width: 26%;
    margin: 0.8rem;
}
#account .dash-prefChoiceOptions.styleGroup .planOptListing {
    justify-content: center;
}
#account .dash-prefChoiceOptions.styleGroup .optionsListing:after {
  display:none;
}

